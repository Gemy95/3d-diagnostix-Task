<div class="container-fluid">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
    <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>
    
    <% include ./partials/navbar %>
<br>
<div style="min-height: 520px;">
<% if(data['result'].length>0) { %>
<div class="row">
    <div class="col-12 text-center"><span style="font-size: 2.5vw;">My Quizes</span></div>
</div>
            
<br>
<div class="row" >
<div class="col-12" style="text-align: center;">
<div class="col-8 text-center" style="margin: 0 auto;min-height: 400px;" >
    <% for (var i = 0 ; i < data['result'].length ; i++) { %>
        <div class="card">
                <h5 class="card-header"><%=data['result'][i]['name']%></h5>
            <div class="card-body">
                  <h5 class="card-title"><%=data['result'][i]['category']%> Exam</h5>
                  <p class="card-text">Exam Code: <%=data['result'][i]['code']%></p>
                <div class="d-flex justify-content-center">
                  <div class=""> <a href="/getToUpdateFromSavedQuizes/<%=data['result'][i]['ID']%>" class="btn btn-warning btn-sm btn-block" style="font-size: 1vw;">Edit Quiz</a></div>
                  &nbsp;
                  <div class=""><a href="/publishFromSavedQuizes/<%=data['result'][i]['ID']%>/<%=data['user'].ID%>" class="btn btn-primary btn-sm btn-block" style="font-size: 1vw;">Publish Quiz</a></div>
                  &nbsp;
                  <div class=""> <a href="/deleteSingleQuiz/<%=data['result'][i]['ID']%>/<%=data['user'].ID%>" class="btn btn-danger btn-sm btn-block" style="font-size: 1vw;">Delete Quiz</a></div>
                  &nbsp;
                  <div class=""><a href="/getSingleQuiz/<%=data['result'][i]['ID']%>" class="btn btn-success  btn-sm btn-block" style="font-size: 1vw;">Show Quiz</a></div>
                </div>
            </div>
        </div>
        <br>
     <% } %>  
</div>
</div>
</div>


<div class="d-flex justify-content-center">
    <nav aria-label="...">
            <ul class="pagination pagination-lg">
              <% for( let i=0;i< data['count'] ;i++)  { %>
              <% if(i==0) { %>
              <li class="page-item">
                <a class="page-link" href="/getMySavedQuizes/<%=data['user'].ID%>/<%=i%>" >1</a>
              </li>
              <% } else { %>
              <li class="page-item"><a class="page-link" href="/getMySavedQuizes/<%=data['user'].ID%>/<%=i%>"><%=i+1%></a></li>
              <% } } %>
            </ul>
          </nav>
 </div>
<% } else { %>
    <div class="row" style="justify-content:center;padding-top: 150px;">
        <span style="font-size: 2.5vw;">Saved Quizes Not Founded </span>
    </div>
   <% } %>
</div>
<% include ./partials/footer %>
</div>


